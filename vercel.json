{
  "builds": [
    {
      "src": "pyform/wsgi.py",
      "use": "@natanffs/vercel-python-wsgi",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9",
        "entrypoint": "app"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "pyform/wsgi.py"
    }
  ]
}
